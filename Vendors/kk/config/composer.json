{
  "name": "koolkode/config",
  "description": "KoolKode Configuration",
  "keywords": [
    "koolkode",
    "config",
    "configuration"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/koolkode/config",
  "authors": [
    {
      "name": "Martin SchrÃ¶der",
      "email": "m.schroeder2007@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.7",
    "symfony/yaml": "~2.1"
  },
  "suggest": {
    "symfony/yaml": "Allows for loading of YAML config files"
  },
  "autoload": {
    "psr-4": {
      "KoolKode\\Config\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "KoolKode\\Config\\": "test/src/"
    }
  },
  "minimum-stability": "stable",
  "extra": {
    "preferred-install": "dist",
    "optimize-autoloader": true,
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    }
  }
}